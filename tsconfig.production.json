{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "types": [
      "node"
    ],
    "module": "ESNext",
    "moduleResolution": "Node10",
    "rootDir": "src",
    "outDir": "dist",
    "outFile": "index.js",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "importHelpers": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}